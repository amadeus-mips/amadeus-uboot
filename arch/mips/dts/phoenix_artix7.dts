/dts-v1/;

/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "phoenix,artix7";

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu@0 {
			device_type = "cpu";
			compatible = "mips,m14Kc";
			clocks	= <&ext>;
			reg = <0>;
		};
	};

	ext: ext {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};
};

/ {
	compatible = "phoenix,artix7";

	/** this is the ddr3 ram */
	memory: memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	cpuintc: interrupt-controller@bd000000 {
		#address-cells = <0>;
		#interrupt-cells = <1>;
                reg = <0xbd000000 0x20>;
		interrupt-controller;
		compatible = "mti,cpu-interrupt-controller";
	};

	aliases {
		console = &serial0;
                serial = &serial0;
		spi0 = &spi_flash;
        //        eth = &axi_eternetlite;
	};

	serial0: serial@bfe40000 {
		compatible = "ns16550a";
		reg = <0xbfe40000 0x4000>;
                reg-offset = <0>;
		reg-io-width = <1>;
                reg-shift = <0>;
                clock-frequency = <33000000>;
                current-speed = <57600>;
		interrupts = <5 0>;
                interrupt-parent = <&cpuintc>;
	};
	spi_flash: spi@bfc00000 {
		device-type = "memory";
		reg = <0xbfc00000 0x1000000>;

	};

